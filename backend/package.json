{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "bin": "dist/start.js",
  "pkg": {
    "scripts": "dist/**/*.js",
    "assets": [
      "dist/**/*",
      "node_modules/**/*"
    ],
    "targets": [
      "node18-linux-x64",
      "node18-win-x64",
      "node18-macos-x64"
    ],
    "outputPath": "dist-bin"
  },
  "scripts": {
    "dev": "nodemon",
    "start": "ts-node --transpile-only -r tsconfig-paths/register -r dotenv/config src/start.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "format": "prettier --write .",
    "prepare": "husky",
    "build": "tsc && tsc-alias",
    "build:bin": "rm -rf dist dist-bin && npm run build && pkg . && cp node_modules/better-sqlite3/build/Release/better_sqlite3.node dist-bin/",
    "commit": "commit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "-": "^0.0.1",
    "@arcjet/node": "^1.0.0-beta.4",
    "@lancedb/lancedb": "^0.26.2",
    "@types/chokidar": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mongoose": "^5.11.97",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "better-sqlite3": "^12.6.2",
    "chokidar": "^5.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "express": "^4.19.2",
    "express-rate-limit": "^8.0.1",
    "fastq": "^1.20.1",
    "fs-extra": "^11.3.3",
    "helmet": "^8.1.0",
    "js-tiktoken": "^1.0.21",
    "jsonwebtoken": "^9.0.3",
    "mongoose": "^8.5.1",
    "multer": "^2.0.2",
    "node-fetch": "^2.7.0",
    "save-dev": "^0.0.1-security",
    "sqlite3": "^5.1.7",
    "tough-cookie": "^6.0.0",
    "ts-node": "^10.9.2",
    "winston": "^3.17.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.19",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.12",
    "@types/multer": "^2.0.0",
    "@types/node-fetch": "^2.6.13",
    "@types/sqlite3": "^3.1.11",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.5.0",
    "nodemon": "^3.1.4",
    "pkg": "^5.8.1",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.3",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0"
  }
}
